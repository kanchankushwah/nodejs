<%- include('includes/head.ejs')%>

<%- include('includes/navigation.ejs')%>
<main>
    <h1>Users</h1>
    <% if (users.length >0) { %>
    <ul>
        <% users.forEach((user)=>{ %>
            <li><%= user.name %></li>
        <% }) %>
    </ul>
    <% } else {%>
        <h1>No User Found!</h1>
    <% } %>
</main>   
 
    
<%- include('includes/end.ejs')%>